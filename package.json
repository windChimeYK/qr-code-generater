{
  "name": "qr-code-generater",
  "description": "Vueを活用してQRコード生成してみよう！（思いつき",
  "private": true,
  "scripts": {
    "watch:js": "webpack ./src/js/entrypoint.js -o ./docs/assets/js/main.js -w --module-bind js=babel-loader --mode development",
    "build:js": "webpack ./src/js/entrypoint.js -o ./docs/assets/js/main.js --module-bind js=babel-loader --mode production",
    "watch:sass": "node-sass --importer node_modules/node-sass-globbing/index.js ./src/sass/entrypoint.scss ./docs/assets/css/main.css -w --output-style compressed",
    "build:sass": "node-sass --importer node_modules/node-sass-globbing/index.js ./src/sass/entrypoint.scss ./docs/assets/css/main.css --output-style compressed",
    "watch": "npm-run-all -p watch:*",
    "build": "npm-run-all -p build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windChimeYK/qr-code-generater.git"
  },
  "author": "WindChimeYK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windChimeYK/qr-code-generater/issues"
  },
  "homepage": "https://github.com/windChimeYK/qr-code-generater#readme",
  "dependencies": {
    "qrcode": "^1.2.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.9.3",
    "node-sass-globbing": "0.0.23",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.1.0"
  }
}
